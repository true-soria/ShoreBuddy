<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/lakeSelectFragment">

    <fragment
        android:id="@+id/lakeSelectFragment"
        android:name="com.example.shorebuddy.views.LakeSelectFragment"
        tools:layout="@layout/fragment_lake_select"
        android:label="LakeSelectFragment" >
        <action
            android:id="@+id/action_lakeSelectFragment_to_mainFragment"
            app:destination="@+id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.shorebuddy.views.MainFragment"
        tools:layout="@layout/main_fragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_lakeSelectFragment"
            app:destination="@id/lakeSelectFragment" />
    </fragment>
</navigation>